<service name="WSRecuperarDatos" >
	<Description>Este WebService permite recuperar data desde la respuesta de un WS Kaanbal.</Description>
	<messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only" class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver" />
		<messageReceiver  mep="http://www.w3.org/2004/08/wsdl/in-out"  class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</messageReceivers>
	<parameter name="ServiceClass" locked="false">com.aje.apm.webservices.WSRecuperarDatos</parameter>
</service>
